<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>/alumno/inicio-sesion.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>dashboard</from-outcome>
            <to-view-id>/alumno/dashboard.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/alumno/dashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>tareas-alumno</from-outcome>
            <to-view-id>/alumno/activities.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>biblioteca-alumno</from-outcome>
            <to-view-id>/alumno/free-library.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>mapametal-movil</from-outcome>
            <to-view-id>/alumno/select-mindmap.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>dashboard</from-outcome>
            <to-view-id>/alumno/dashboard.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/alumno/activities.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>transcript</from-outcome>
            <to-view-id>/alumno/activity.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>biblioteca-alumno</from-outcome>
            <to-view-id>/alumno/free-library.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>dashboard</from-outcome>
            <to-view-id>/alumno/dashboard.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>mapametal-movil</from-outcome>
            <to-view-id>/alumno/select-mindmap.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/alumno/free-library.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>dashboard</from-outcome>
            <to-view-id>/alumno/dashboard.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/alumno/mindmap/*</from-view-id>
        <navigation-case>
            <from-outcome>dashboard</from-outcome>
            <to-view-id>/alumno/dashboard.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/alumno/select-mindmap.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>mapametal</from-outcome>
            <to-view-id>/alumno/select-mindmap.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>biblioteca-alumno</from-outcome>
            <to-view-id>/alumno/free-library.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/alumno/activity.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>dashboard</from-outcome>
            <to-view-id>/alumno/dashboard.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>actividades</from-outcome>
            <to-view-id>/alumno/activities.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>glosario</from-outcome>
            <to-view-id>/alumno/glossary.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>mapa-mental</from-outcome>
            <to-view-id>/alumno/select-mindmap.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>biblioteca-alumno</from-outcome>
            <to-view-id>/alumno/free-library.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>