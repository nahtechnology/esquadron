<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>profesor</from-outcome>
            <to-view-id>/profesor/dashboard.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    <navigation-rule>
        <from-view-id>/profesor/dashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>alumnos</from-outcome>
            <to-view-id>/profesor/admin-alumnos.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>buzon</from-outcome>
            <to-view-id>/profesor/buzon.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>biblioteca-profesor</from-outcome>
            <to-view-id>/profesor/dashboard.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>perfil</from-outcome>
            <to-view-id>/profesor/dashboard.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>asignar-tarea</from-outcome>
            <to-view-id>/profesor/asignar-actividad.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/profesor/admin-alumnos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>tablero</from-outcome>
            <to-view-id>/profesor/dashboard.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>califica-mapamental</from-outcome>
            <to-view-id>/profesor/calificacion-mapamental.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>detalle-alumno</from-outcome>
            <to-view-id>/profesor/tarea-alumno.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>agregar-csv</from-outcome>
            <to-view-id>/profesor/agregar-csv.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>asignar-tarea</from-outcome>
            <to-view-id>/profesor/asignar-actividad.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/profesor/tarea-alumno.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>gramatica</from-outcome>
            <to-view-id>/profesor/calificacion-gramatica.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>mapamental</from-outcome>
            <to-view-id>/profesor/lista-mapa-mental.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>dashboard-maestro</from-outcome>
            <to-view-id>/profesor/dashboard.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>admin-alumnos</from-outcome>
            <to-view-id>/profesor/admin-alumnos.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>tablero</from-outcome>
            <to-view-id>/profesor/admin-alumnos.xhtml</to-view-id>
            <redirect>
                <redirect-param>
                    <name>grupo</name>
                    <value>#{profesorGrupoControlador.grupoModelo.id}</value>
                </redirect-param>
            </redirect>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/profesor/lista-mapa-mental.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>califica</from-outcome>
            <to-view-id>/profesor/calificacion-mapamental.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>dashboard-maestro</from-outcome>
            <to-view-id>/profesor/dashboard.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>tablero</from-outcome>
            <to-view-id>/profesor/tarea-alumno.xhtml</to-view-id>
            <redirect>
                <redirect-param>
                    <name>idAlumno</name>
                    <value>#{profesorGrupoControlador.alumnoModelo.id}</value>
                </redirect-param>
            </redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>admin-alumnos</from-outcome>
            <to-view-id>/profesor/admin-alumnos.xhtml</to-view-id>
            <redirect>
                <redirect-param>
                    <name>grupo</name>
                    <value>#{profesorGrupoControlador.grupoModelo.id}</value>
                </redirect-param>
            </redirect>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/profesor/calificacion-mapamental.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>dashboard-maestro</from-outcome>
            <to-view-id>/profesor/dashboard.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>tareas</from-outcome>
            <to-view-id>/profesor/lista-mapa-mental.xhtml</to-view-id>
            <redirect include-view-params="true">
                <redirect-param>
                    <name>idAlumno</name>
                    <value>#{profesorGrupoControlador.alumnoModelo.id}</value>
                </redirect-param>
            </redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>tablero</from-outcome>
            <to-view-id>/profesor/tarea-alumno.xhtml</to-view-id>
            <redirect>
                <redirect-param>
                    <name>idAlumno</name>
                    <value>#{profesorGrupoControlador.alumnoModelo.id}</value>
                </redirect-param>
            </redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>admin-alumnos</from-outcome>
            <to-view-id>/profesor/admin-alumnos.xhtml</to-view-id>
            <redirect>
                <redirect-param>
                    <name>grupo</name>
                    <value>#{profesorGrupoControlador.grupoModelo.id}</value>
                </redirect-param>
            </redirect>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/profesor/calificacion-gramatica.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>tareas</from-outcome>
            <to-view-id>/profesor/tarea-alumno.xhtml</to-view-id>
            <redirect include-view-params="true">
                <redirect-param>
                    <name>idAlumno</name>
                    <value>#{profesorGrupoControlador.alumnoModelo.id}</value>
                </redirect-param>
            </redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>dashboard-maestro</from-outcome>
            <to-view-id>/profesor/dashboard.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>tablero</from-outcome>
            <to-view-id>/profesor/tarea-alumno.xhtml</to-view-id>
            <redirect>
                <redirect-param>
                    <name>idTarea</name>
                    <value>#{calificacionGramaticaControlador.idTarea}</value>
                </redirect-param>
            </redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>admin-alumnos</from-outcome>
            <to-view-id>/profesor/admin-alumnos.xhtml</to-view-id>
            <redirect>
                <redirect-param>
                    <name>grupo</name>
                    <value>#{profesorGrupoControlador.grupoModelo.id}</value>
                </redirect-param>
            </redirect>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/profesor/asignar-actividad.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>dashboard</from-outcome>
            <to-view-id>/profesor/dashboard.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>todas-actividades</from-outcome>
            <to-view-id>/profesor/asignar-actividad-buscar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>admin-alumnos</from-outcome>
            <to-view-id>/profesor/admin-alumnos.xhtml</to-view-id>
            <redirect>
                <redirect-param>
                    <name>grupo</name>
                    <value>#{asignarActividadControlador.grupo}</value>
                </redirect-param>
            </redirect>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/profesor/agregar-csv.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>regresar</from-outcome>
            <to-view-id>/profesor/admin-alumnos.xhtml</to-view-id>
            <redirect>
                <redirect-param>
                    <name>grupo</name>
                    <value>#{profesorGrupoControlador.grupoModelo.id}</value>
                </redirect-param>
            </redirect>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/profesor/buzon.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>regresar</from-outcome>
            <to-view-id>/profesor/dashboard.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/profesor/asignar-actividad-buscar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>asignar</from-outcome>
            <to-view-id>/profesor/dashboard.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>