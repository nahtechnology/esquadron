<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
			  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
			  version="2.2">
    
    <application>
    	<resource-bundle>
    		<base-name>lenguaje.comun</base-name>
    		<var>mensaje</var>
    	</resource-bundle>
		<resource-bundle>
			<base-name>lenguaje.boton</base-name>
			<var>boton</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>lenguaje.escuela-dashboard</base-name>
			<var>escuela-dashboard</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>lenguaje.escuela-detalle</base-name>
			<var>escuela-detalle</var>
		</resource-bundle>
    </application>

	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>admin</from-outcome>
			<to-view-id>/administracion/dashboard.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
